programa
{
	
	funcao inicio()
	{
	inteiro velocidade_carro , excesso_velocidade
		inteiro multa
	
		escreva("Em qual velocidade o carro está: ")
		leia(velocidade_carro)
 
		se(velocidade_carro > 80){
			escreva("Você está sendo multado por excesso de velocidade!\n")

			excesso_velocidade = velocidade_carro - 80
			multa = excesso_velocidade * 7
			escreva("O valor da multa é R$", multa)
		}
		}
	}
