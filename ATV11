programa
{
	
	funcao inicio()
	{
	
		inteiro nota, nota2, media
		
		escreva("Informe sua primeira nota: ")
		leia(nota)

		escreva("Informe sua segunda nota: ")
		leia(nota2)


		media= (nota+nota2)/2

		se(media == 10){
			escreva("Aprovado com distinção!")
		}
		senao se(media >= 7){
			escreva("Aprovado!")
		}
		senao{
			escreva("Reprovado!")
		}
	}
}
